{
    "problemMatcher": [
        {
            "owner": "perl",
            "pattern": [
                {
                    "regexp": "/^#\\s*(.+) at (.+) line (\\d+)\\.$/",
                    "file": 2,
                    "line": 3,
                    "message": 1,
                    "loop": true
                }
            ]
        }
    ]
}
