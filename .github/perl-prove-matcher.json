{
    "problemMatcher": [
        {
            "owner": "perl",
            "pattern": [
                {
                    "regexp": "^#\\s*(.+) at (.+) line (.+)\\.$",
                    "file": 2,
                },
                {
                    "regexp": "^#\\s*(.+)$",
                    "message": 1,
                    "loop": true
                }
            ]
        }
    ]
}
